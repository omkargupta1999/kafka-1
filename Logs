      - name: microservices-dev-pull-secret
      tolerations:
      - effect: NoSchedule
        key: workload-type
        operator: Equal
        value: kafka
  tls:
    trustedCertificates:
    - pattern: '*.crt'
      secretName: dev-kafka-cluster-cluster-ca-cert
